@using KiraNet.CodeGenerator
@model KiraNet.CodeGenerator.ModelEntity
namespace @Model.Namespace
{
    public partial class @Model.Name@((!string.IsNullOrEmpty(Model.Parent)) ? " : " + Model.Parent : "")
    {
    @foreach (var property in Model.Properties)
    {
        <text>public virtual @property.Type.Name @property.Name { get; set; } @(property.DefaultValue != null && !Util.TryGetDefaultValueString(property.Type,property.DefaultValue, out string value)? (" = " value + ";"):"")</text>
    }
    }
    }